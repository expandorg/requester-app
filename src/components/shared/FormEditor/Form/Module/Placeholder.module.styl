@import 'components/common/uikit/colors'


.placehoder
  box-sizing: border-box;

  height: 60px;
  line-height: 56px

  font-family: 'Avenir';
  font-weight: 800;
  font-size: 11px;
  letter-spacing: 0.2em;

  text-align: center;
  text-transform: uppercase;

  color: #5497E2;
  background-color: color-medium-blue;
  border: 2px dashed color-heavy-blue;
  border-radius: 5px;
